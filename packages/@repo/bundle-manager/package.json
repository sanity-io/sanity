{
  "name": "@repo/bundle-manager",
  "version": "4.10.1",
  "private": true,
  "description": "Internal tool for building and uploading bundles to modules.sanity-cdn.com",
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "bundle-manager": "./bin/bundle-manager.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "check:types": "(cd ../../.. && tsc --project packages/@repo/bundle-manager/tsconfig.json --erasableSyntaxOnly)",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@repo/utils": "workspace:*",
    "lodash-es": "^4.17.21",
    "read-package-up": "^11.0.0",
    "semver": "^7.7.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/test-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@repo/utils": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.19 <22 || >=22.12"
  }
}
