{
  "$schema": "https://typedoc.org/schema.json",
  "name": "sanity",
  "entryPoints": [
    "./src/_exports/index.ts",
    "./src/_exports/structure.ts",
    "./src/_exports/presentation.ts",
    "./src/_exports/router.ts",
    "./src/_exports/media-library.ts"
  ],
  "entryPointStrategy": "expand",
  "json": "./docs/sanity-typedoc-output.json",
  "tsconfig": "./tsconfig.docs.json",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeProtected": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "kindSortOrder": [
    "Class",
    "Function",
    "Interface",
    "Module",
    "Namespace",
    "Project",
    "Enum",
    "EnumMember",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature",
    "Reference"
  ],
  "searchInComments": true,
  "includeVersion": true,
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/sanity-io/sanity/blob/{gitRevision}/{path}#L{line}",
  "githubPages": false,
  "emit": "docs",
  "commentStyle": "jsdoc"
}
