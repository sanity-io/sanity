    "is-hotkey-esm": "^1.0.0",
    "json-2-csv": "^5.5.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "quick-lru": "^5.1.1",
    "react-compiler-runtime": "19.1.0-rc.2",
