    "hashlru": "^2.3.0",
    "is-hotkey": "^0.1.6",
    "json5": "^2.2.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@sanity/client": "^6.4.9",
    "react": "^18.2.0",
    "sanity": "3.18.1",
