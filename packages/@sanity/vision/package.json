{
  "name": "@sanity/vision",
  "version": "5.12.0",
  "description": "Sanity plugin for running/debugging GROQ-queries against Sanity datasets",
  "keywords": [
    "admin",
    "cms",
    "content",
    "gui",
    "headless",
    "realtime",
    "sanity",
    "sanity-plugin",
    "vision"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/vision"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "monorepo": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./lib/index.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "pkg-utils build --strict --check --clean",
    "build:bundle": "vite build --configLoader runner --config package.bundle.ts",
    "check:types": "(cd ../../.. && tsgo --project packages/@sanity/vision/tsconfig.lib.json --erasableSyntaxOnly)",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "prepublishOnly": "turbo run build",
    "test": "vitest",
    "watch": "pkg-utils watch"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.12.1",
    "@codemirror/search": "^6.6.0",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.11",
    "@lezer/highlight": "^1.2.3",
    "@rexxars/react-json-inspector": "^9.0.1",
    "@rexxars/react-split-pane": "^1.0.0",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.4",
    "@sanity/ui": "catalog:",
    "@sanity/uuid": "^3.0.2",
    "@uiw/react-codemirror": "^4.25.4",
    "is-hotkey-esm": "^1.0.0",
    "json-2-csv": "^5.5.9",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.22",
    "quick-lru": "^5.1.1",
    "react-fast-compare": "^3.2.2",
    "react-rx": "^4.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/package.bundle": "workspace:*",
    "@repo/package.config": "workspace:*",
    "@repo/test-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@sanity/client": "catalog:",
    "@sanity/eslint-config-i18n": "catalog:",
    "@sanity/pkg-utils": "catalog:",
    "@testing-library/react": "^16.3.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "babel-plugin-react-compiler": "1.0.0",
    "babel-plugin-styled-components": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "^5.0.10",
    "sanity": "^5.12.0",
    "styled-components": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "^19.2.2",
    "sanity": "^4.0.0-0 || ^5.0.0-0",
    "styled-components": "^6.1.15"
  }
}
