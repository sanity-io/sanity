    "@sanity/observable": "^0.103.13",
    "@sanity/schema": "^0.103.13",
    "arrify": "^1.0.1",
    "canvas-to-blob": "^0.0.0",
    "debug": "^2.6.1",
    "exif-component": "^1.0.1",
    "get-random-values": "^1.2.0",
    "get-window": "^1.1.1",
    "humanize-list": "^1.0.1",
    "immutable": "^3.8.1",
    "is-empty": "^1.0.0",
    "lodash": "^4.17.4",
    "object-inspect": "^1.2.1",
    "proptypes": "^0.14.3",
    "prosemirror": "^0.10.1",
    "react-portal": "^3.0.0",
    "react-sortable-hoc": "^0.4.5",
    "shallow-equals": "^1.0.0",
    "slate": "^0.19.3",
    "slate-insert-block-on-enter": "0.0.2",
    "slate-soft-break": "^0.3.0",
    "speakingurl": "^13.0.0"
  },
  "devDependencies": {
