    "get-random-values": "^1.2.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@sanity/schema": "0.125.0",
    "assert": "^1.4.1",
    "in-publish": "^2.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jsdom": "^11.3.0",
    "xpath": "^0.0.24"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "bugs": {
