    "@types/diff-match-patch": "^1.0.32",
    "debug": "^3.1.0",
    "diff-match-patch": "^1.0.4",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "rimraf": "^2.7.1",
    "tap": "^14.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git"
  },
  "bugs": {
