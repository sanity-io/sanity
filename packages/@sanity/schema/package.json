{
  "name": "@sanity/schema",
  "version": "5.8.0",
  "description": "",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "schema"
  ],
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/schema"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/_exports/index.ts",
      "monorepo": "./src/_exports/index.ts",
      "default": "./lib/index.js"
    },
    "./_internal": {
      "source": "./src/_exports/_internal.ts",
      "monorepo": "./src/_exports/_internal.ts",
      "default": "./lib/_internal.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "_internal": [
        "./lib/_internal.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm clean && pkg-utils build --strict --check --clean",
    "clean": "rimraf _internal.js lib",
    "lint": "eslint .",
    "prepublishOnly": "turbo run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "watch": "pkg-utils watch"
  },
  "dependencies": {
    "@sanity/descriptors": "^1.3.0",
    "@sanity/generate-help-url": "^4.0.0",
    "@sanity/types": "workspace:*",
    "arrify": "^2.0.1",
    "groq-js": "^1.26.0",
    "humanize-list": "^1.0.1",
    "leven": "^3.1.0",
    "lodash-es": "^4.17.22",
    "object-inspect": "^1.13.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/package.config": "workspace:*",
    "@repo/test-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@sanity/icons": "^3.7.4",
    "@sanity/pkg-utils": "catalog:",
    "@types/lodash-es": "^4.17.12",
    "@types/object-inspect": "^1.13.0",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "rimraf": "^5.0.10",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "exports": {
      ".": "./lib/index.js",
      "./_internal": "./lib/_internal.js",
      "./package.json": "./package.json"
    }
  }
}
