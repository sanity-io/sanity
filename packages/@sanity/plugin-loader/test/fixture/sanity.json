{
  "plugins": [
    "date",
    "better-date"
  ],

  "env": {
    "development": {
      "fromEnv": true
    }
  },

  "parts": [
    {
      "name": "part:base/bar",
      "description": "Returns 'bar' when called",
      "path": "./getBar.js"
    },
    {
      "name": "part:base/indexpart",
      "description": "Points to an index",
      "path": "./folder"
    }
  ],

  "locale": {
    "supportedLanguages": ["en-US", "no-NB"]
  }
}
