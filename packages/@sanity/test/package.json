{
  "name": "@sanity/test",
  "version": "3.19.1",
  "description": "Sanity Testing Package",
  "private": true,
  "keywords": [
    "playwright",
    "sanity",
    "testing"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/types"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./lib/dts/src/index.d.ts",
      "source": "./src/index.ts",
      "require": "./lib/src/index.js",
      "node": {
        "import": "./lib/index.cjs.mjs"
      },
      "import": "./lib/index.esm.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/src/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/src/index.d.ts",
  "bin": {
    "sanity-test": "./bin/sanity-test"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "run-s clean",
    "build": "tsc --project tsconfig.lib.json",
    "check:package": "pkg-utils --strict --tsconfig tsconfig.lib.json",
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "watch": "tsc --watch --project tsconfig.lib.json"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "@playwright/test": "^1.39.0",
    "@sanity/uuid": "^3.0.1",
    "@sanity/client": "^6.7.0"
  }
}
