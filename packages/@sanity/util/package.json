{
  "name": "@sanity/util",
  "version": "3.0.0-dev-preview.14",
  "description": "Utilities shared across projects of Sanity",
  "source": "./src/index.ts",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/dts/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    },
    "./fs": {
      "source": "./src/fsTools.ts",
      "require": "./lib/fsTools.cjs",
      "default": "./lib/fsTools.js"
    },
    "./content": {
      "source": "./src/content/index.ts",
      "require": "./lib/content.cjs",
      "default": "./lib/content.js"
    },
    "./legacyDateFormat": {
      "source": "./src/legacyDateFormat.ts",
      "require": "./lib/legacyDateFormat.cjs",
      "default": "./lib/legacyDateFormat.js"
    },
    "./paths": {
      "source": "./src/paths.ts",
      "require": "./lib/paths.cjs",
      "default": "./lib/paths.js"
    }
  },
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "fs": [
        "./lib/dts/src/fsTools.d.ts"
      ],
      "content": [
        "./lib/dts/src/content/index.d.ts"
      ],
      "legacyDateFormat": [
        "./lib/dts/src/legacyDateFormat.d.ts"
      ],
      "paths": [
        "./lib/dts/src/paths.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json",
    "clean": "rimraf lib",
    "exports:check": "../../../bin/pkg-utils exports-check --target node",
    "exports:clean": "../../../bin/pkg-utils exports-clean",
    "exports:generate": "../../../bin/pkg-utils exports-generate",
    "postbuild": "run-s exports:generate",
    "test": "jest",
    "watch": "../../../bin/pkg-utils bundle --target node --tsconfig tsconfig.lib.json --watch"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/util"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "util"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "dependencies": {
    "@sanity/types": "3.0.0-dev-preview.14",
    "get-random-values-esm": "^1.0.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "rimraf": "^3.0.2"
  }
}
