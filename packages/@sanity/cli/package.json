{
  "name": "@sanity/cli",
  "version": "0.99.7",
  "description": "Sanity CLI tool for managing Sanity installations, managing plugins, schemas and datasets",
  "main": "lib/cli.js",
  "bin": {
    "sanity": "./bin/sanity.js"
  },
  "scripts": {
    "build": "npm test && npm run package-yarn",
    "package-yarn": "node -r babel-register src/scripts/package-yarn.js",
    "test": "mocha test/**/*.test.js",
    "clean": "rimraf lib vendor/yarn",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "posttest": "eslint ."
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/cli.git"
  },
  "keywords": [
    "sanity",
    "cli",
    "tool"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanity-io/cli/issues"
  },
  "homepage": "https://github.com/sanity-io/cli#readme",
  "dependencies": {
    "@sanity/client": "^0.99.7",
    "@sanity/resolver": "^0.99.0",
    "@sanity/util": "^0.99.0",
    "babel-polyfill": "^6.20.0",
    "chalk": "^1.1.3",
    "configstore": "^2.0.0",
    "debug": "^2.6.0",
    "deep-sort-object": "^1.0.1",
    "execa": "^0.6.0",
    "fs-promise": "^1.0.0",
    "git-user-info": "^1.0.0",
    "gitconfiglocal": "^2.0.1",
    "github-url-to-object": "^2.1.0",
    "in-publish": "^2.0.0",
    "inquirer": "^2.0.0",
    "is-git-url": "^0.2.3",
    "leven": "^2.0.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "opn": "^4.0.2",
    "ora": "^1.0.0",
    "osenv": "^0.1.4",
    "package-json": "^2.4.0",
    "progrescii": "^0.1.1",
    "promise-props-recursive": "^1.0.0",
    "resolve-from": "^2.0.0",
    "semver-compare": "^1.0.0",
    "simple-get": "^2.4.0",
    "split2": "^2.1.1",
    "thenify": "^3.2.0",
    "update-notifier": "^1.0.3",
