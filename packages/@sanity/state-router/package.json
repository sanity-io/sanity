{
  "name": "@sanity/state-router",
  "version": "2.14.0",
  "description": "A path pattern => state object bidirectional mapper",
  "main": "./lib/index.js",
  "types": "./dist/dts",
  "//": "the typesVersion config below is a workaround for TypeScript's lack of support for package exports",
  "typesVersions": {
    "*": {
      "dist/dts/index.d.ts": [
        "dist/dts/index.d.ts"
      ],
      "components": [
        "dist/dts/components/index.d.ts"
      ]
    }
  },

  "scripts": {
    "start": "remon --require ts-node/register demo-server/app.ts",
    "test": "jest",
    "compile:watch": "babel --watch --out-dir lib/ src/",
    "benchmark": "npm run compile && node --prof --logfile=benchmarks.log perf/benchmark.js",
    "clean": "rimraf lib dest"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "state-router"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.2.7",
    "lodash": "^4.17.15",
    "nano-pubsub": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "browserify": "^14.3.0",
    "error-capture-middleware": "0.0.2",
    "express": "^4.16.1",
    "history": "^4.6.3",
    "jest": "^26.6.3",
    "object-inspect": "^1.6.0",
    "prop-types": "^15.6.0",
    "quickreload": "^2.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rebundler": "^0.3.0",
    "remon": "^1.0.2",
    "rimraf": "^2.7.1",
    "staticr": "^4.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsify": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^16.9 || ^17",
    "react-dom": "^16.9 || ^17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/state-router"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/"
}
