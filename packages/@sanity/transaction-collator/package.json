{
  "name": "@sanity/transaction-collator",
  "version": "2.2.6",
  "description": "Collate events from a series of transactions",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Sanity.io <hello@sanity.io>",
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "clean": "rimraf lib coverage"
  },
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "transaction-collator",
    "mutations",
    "history"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^26.4.2",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "ts-jest": "^26.3.0",
    "tslint": "^5.18.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/sanity.git",
    "directory": "packages/@sanity/transaction-collator"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity/issues"
  },
  "homepage": "https://www.sanity.io/",
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^part:@sanity/base/schema$": "<rootDir>/test/mocks/schema.js",
      "^part:@sanity/base/client$": "<rootDir>/test/mocks/client.js"
    },
    "testPathIgnorePatterns": [
      "/lib/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/lib/"
    ]
  }
}
