{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "semi": 0,
    "eqeqeq": 0,
    "quotes": 0,
    "no-console": 1,
    "no-var": 2,
    "no-alert": 2,
    "new-cap": [2, {"capIsNew": false}],
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}]
  }
}
