{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "OS",
    "RUNNER_OS",
    "PKG_FILE_PATH",
    "PKG_FORMAT",
    "PKG_RUNTIME",
    "PKG_VERSION",
    "DEBUG"
  ],
  "globalDependencies": [
    ".eslintrc.cjs",
    ".prettierrc",
    "lerna.json"
  ],
  "pipeline": {
    "build": {
      "outputs": [
        "lib/**",
        "*.js"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputMode": "new-only"
    },
    "build:bundle": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "build"
      ],
      "outputMode": "new-only"
    },
    "check:types": {
      "outputs": [],
      "dependsOn": [
        "^build"
      ],
      "outputMode": "errors-only"
    },
    "lint": {
      "outputMode": "errors-only"
    },
    "//#check:format": {}
  }
}
