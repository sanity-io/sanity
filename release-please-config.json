    "packages/@sanity/mutator": {"component": "mutator", "include-component-in-tag": false},
    "packages/@sanity/schema": {"component": "schema", "include-component-in-tag": false},
    "packages/@sanity/types": {"component": "types", "include-component-in-tag": false},
    "packages/@sanity/util": {"component": "util", "include-component-in-tag": false},
    "packages/@sanity/vision": {"component": "vision", "include-component-in-tag": false},
    "packages/create-sanity": {"component": "create-sanity", "include-component-in-tag": false},
    "packages/groq": {"component": "groq", "include-component-in-tag": false},
    "packages/sanity": {"component": "sanity", "include-component-in-tag": false}
