{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Sanity",
  "readme": "./README.md",
  "entryPoints": ["./packages/sanity/typedoc/typedoc.json"],
  "entryPointStrategy": "merge",
  "sortEntryPoints": false,
  "categorizeByGroup": false,
  "alwaysCreateEntryPointModule": false,
  "router": "structure",
  "includeVersion": true,
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript",
    "markdown"
  ],
  "navigation": {
    "includeCategories": true,
    "includeFolders": false
  }
}
