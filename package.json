{
  "name": "@sanity/form-builder",
  "version": "0.1.20",
  "description": "Sanity form builder",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src --copy-files --out-dir lib",
    "compile:watch": "npm run compile -- --watch",
    "prepublish": "in-publish && npm run compile && sanity-check || not-in-publish",
    "postpublish": "rimraf lib",
    "lint": "eslint . --quiet",
    "tap": "tap --node-arg -r --node-arg ./test/.init.js",
    "test": "npm run tap ./test/*.test.js ./test/**/*.test.js",
    "start": "npm run example-testbed",
    "example-testbed": "cd examples/schema-testbed && sanity start",
    "example-undo": "cd examples/undo && sanity start",
    "example-quickstart": "cd examples/quickstart && sanity start"
  },
  "keywords": [],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/imagetool": "^0.2.2",
    "@sanity/mutator": "^0.2.5",
    "arrify": "^1.0.1",
    "canvas-to-blob": "0.0.0",
    "debug": "^2.6.0",
    "exif-component": "^1.0.1",
    "get-random-values": "^1.2.0",
    "get-window": "^1.1.1",
    "immutable": "^3.8.1",
    "is-empty": "^1.0.0",
    "lodash": "^4.17.4",
    "object-inspect": "^1.2.1",
    "proptypes": "^0.14.3",
    "prosemirror": "^0.10.1",
    "react-sortable-hoc": "0.4.5",
    "shallow-equals": "0.0.0",
    "slate": "^0.16.9",
    "slate-insert-block-on-enter": "0.0.2"
  },
  "devDependencies": {
    "@sanity/base": "^0.3.12",
    "@sanity/check": "^0.0.3",
    "@sanity/cli": "^0.1.25",
    "@sanity/components": "^0.0.89",
    "@sanity/core": "^0.0.34",
    "@sanity/plugin-loader": "^0.0.4",
    "@sanity/server": "^0.0.22",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-sanity": "^2.0.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "in-publish": "^2.0.0",
    "jsdom": "^9.9.1",
    "json-markup": "^1.1.0",
    "path-to-regexp": "^1.7.0",
    "postcss-cssnext": "^2.9.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-enroute": "^1.0.0",
    "rimraf": "^2.5.4",
    "tap": "^8.0.1",
    "zen-observable": "^0.4.0"
  },
  "peerDependencies": {
    "react": "^15.x",
    "react-dom": "^15.x"
  },
