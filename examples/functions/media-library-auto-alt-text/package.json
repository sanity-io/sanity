{
  "name": "media-library-auto-alt-text",
  "type": "module",
  "main": "index.ts",
  "dependencies": {
    "@sanity/client": "^7.15.0",
    "@sanity/functions": "^1.1.0"
  },
  "blueprintResourceItem": {
    "type": "sanity.function.media-library.asset",
    "name": "media-library-auto-alt-text",
    "src": "./functions/media-library-auto-alt-text",
    "memory": 2,
    "timeout": 30,
    "event": {
      "on": [
        "create",
        "update"
      ],
      "filter": "assetType == 'sanity.imageAsset' && !defined(aspects.altText)",
      "projection": "{ _id, currentVersion }",
      "resource": {
        "type": "media-library",
        "id": "<your-media-library-id>"
      }
    }
  },
  "exampleInstructions": "Add this resource to your Blueprint config's resources array. Go to README.md for more details on how to use this function."
}
