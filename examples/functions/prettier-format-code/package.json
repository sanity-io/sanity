{
  "name": "prettier-format-code",
  "type": "module",
  "main": "index.ts",
  "dependencies": {
    "@sanity/client": "^7.11.0",
    "@sanity/functions": "^1.0.3",
    "prettier": "^3.6.2"
  },
  "blueprintResourceItem": {
    "name": "prettier-format-code",
    "memory": 1,
    "timeout": 10,
    "event": {
      "on": [
        "publish"
      ],
      "filter": "_type == 'article' && delta::changedAny(content[_type == 'code'])",
      "projection": "{_id, content}"
    }
  },
  "exampleInstructions": "Add this resource to your Blueprint config's resources array. Go to README.md for more details on how to use this function."
}
