{
  "private": true,
  "name": "sanity-workshop",
  "version": "2.22.4",
  "main": "package.json",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf parts.ts public",
    "dev": "npm run parts:generate && vite",
    "lint": "eslint . --ext .js,.jsx,.mjs,.ts,.tsx --quiet",
    "parts:generate": "node -r esbuild-register __legacy/gen-parts.ts",
    "prebuild": "npm run clean && npm run parts:generate",
    "start": "http-server -a localhost -c-0 -p 9009 -s -P http://localhost:9009/index.html? public",
    "vercel-build": "npm run parts:generate && npx vite build",
    "type-check": "tsc -b"
  },
  "dependencies": {
    "@sanity/ui": "^0.36.15",
    "@sanity/ui-workshop": "^0.3.5",
    "qs": "^6.10.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-refractor": "^2.1.2",
    "refractor": "^3.3.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "1.0.0",
    "@sanity/base": "2.22.3",
    "@sanity/default-layout": "2.22.4",
    "@sanity/default-login": "2.22.3",
    "@sanity/desk-tool": "2.22.3",
    "@sanity/resolver": "2.22.3",
    "@sanity/types": "2.22.3",
    "@types/qs": "^6.9.7",
    "@types/react-dom": "^17.0.0",
    "@types/refractor": "^3.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "globby": "^10.0.0",
    "postcss-cssnext": "^3.0.2",
    "resolve": "^1.3.3",
    "rimraf": "^2.7.1",
    "vite": "^2.4.4"
  }
}
