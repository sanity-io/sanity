{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "node": "6",
        "chrome": "59",
        "safari": "10",
        "firefox": "56",
        "edge": "14"
      }
    }]
  ],
  "plugins": [
    "lodash",
    "syntax-class-properties",
    "transform-class-properties",
    "syntax-object-rest-spread",
    "transform-object-rest-spread",
    "syntax-flow",
    "transform-flow-comments"
  ]
}
