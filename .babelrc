{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "14",
          "chrome": "61",
          "safari": "11.3",
          "firefox": "60",
          "edge": "79"
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@rexxars/inline-json-import", {"match": "/sanity/package.json"}]
  ]
}
